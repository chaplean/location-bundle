{
    "name": "chaplean/location-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Location bundle contains Entity Region/Department/City",
    "authors": [
        {
            "name": "Chaplean",
            "homepage": "http://www.chaplean.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Chaplean\\Bundle\\LocationBundle": ""
        },
        "classmap": [
            "app/AppKernel.php"
        ]
    },
    "target-dir" : "Chaplean/Bundle/LocationBundle",
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.chaplean.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "chaplean/csv-bundle": "^3.0",
        "friendsofsymfony/rest-bundle": "~1.7",
        "jms/serializer-bundle": "^1.1"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "chaplean/coding-standard": "1.1.*",
        "chaplean/unit-bundle": "~2.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "discard-changes": true,
        "platform": {
            "php": "5.5.9"
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "symlink",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
