{
    "name": "chaplean/location-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Location bundle contains Entity Region/Department/City",
    "authors": [
        {
            "name": "Chaplean",
            "homepage": "http://www.chaplean.coop"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chaplean\\Bundle\\LocationBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Chaplean\\Bundle\\LocationBundle\\": "Tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.chaplean.coop"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "chaplean/csv-bundle": "^6.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "friendsofsymfony/rest-bundle": "^2.1",
        "jms/serializer-bundle": "^1.1 || ^2.0",
        "sensio/framework-extra-bundle": "^3.0",
        "symfony/assetic-bundle": "^2.3",
        "symfony/config": "^2.8 || ^3.0 || ^4.0",
        "symfony/console": "^2.8 || ^3.0 || ^4.0",
        "symfony/dependency-injection": "^2.7 || ^3.0 || ^4.0",
        "symfony/framework-bundle": "^2.7 || ^3.0 || ^4.0",
        "symfony/routing": "^2.7 || ^3.0 || ^4.0",
        "symfony/yaml": "^2.8 || ^3.0 || ^4.0"
    },
    "require-dev": {
        "chaplean/codeship-scripts": "^1.0",
        "chaplean/coding-standard": "^1.1",
        "chaplean/unit-bundle": "^6.0",
        "mockery/mockery": "dev-master",
        "satooshi/php-coveralls": "dev-master",
        "symfony/http-kernel": "^2.8 || ^3.0 || ^4.0",
        "symfony/var-dumper": "^2.8 || ^3.0 || ^4.0"
    },
    "config": {
        "bin-dir": "bin",
        "discard-changes": true,
        "sort-packages": true
    }
}
