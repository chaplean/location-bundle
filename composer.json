{
    "name": "chaplean/location-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Location bundle contains Entity Region/Department/City",
    "authors": [
        {
            "name": "Chaplean",
            "homepage": "http://www.chaplean.coop"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chaplean\\Bundle\\LocationBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Chaplean\\Bundle\\LocationBundle\\": "Tests/"
        },
        "classmap": [
            "app/AppKernel.php"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.chaplean.coop"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "chaplean/csv-bundle": "^7.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "sensio/framework-extra-bundle": "^3.0 || ^4.0 || ^5.0",
        "symfony/config": "^3.0 || ^4.0",
        "symfony/console": "^3.0 || ^4.0",
        "symfony/dependency-injection": "^3.0 || ^4.0",
        "symfony/yaml": "^3.0 || ^4.0",
        "symfony/serializer": "^3.0 || ^4.0"
    },
    "require-dev": {
        "chaplean/coding-standard": "^1.1",
        "chaplean/unit-bundle": "^7.0",
        "mockery/mockery": "dev-master",
        "symfony/monolog-bundle": "~2.4",
        "symfony/http-kernel": "^3.0 || ^4.0",
        "symfony/var-dumper": "^3.0 || ^4.0",
        "symfony/phpunit-bridge": "^4.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var"
    }
}
